<metadata>
  <remove-node path="/api/package[starts-with(@name, 'com.actionbarsherlock.internal')]" />
  
  <!-- Canonicalize the Java package names -->
  <attr path="/api/package[@name='com.actionbarsherlock']" name="managedName">Xamarin.ActionBarSherlock</attr>
  <attr path="/api/package[@name='com.actionbarsherlock.widget']" name="managedName">Xamarin.ActionBarSherlock.Widget</attr>
  <attr path="/api/package[@name='com.actionbarsherlock.app']" name="managedName">Xamarin.ActionBarSherlock.App</attr>
  <attr path="/api/package[@name='com.actionbarsherlock.view']" name="managedName">Xamarin.ActionBarSherlock.Views</attr>
</metadata>